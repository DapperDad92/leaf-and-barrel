{
  "name": "leaf-and-barrel",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "npm run check:path && expo run:ios",
    "web": "expo start --web",
    "dev:ios": "npm run check:path && expo run:ios",
    "dev:android": "expo run:android",
    "prebuild": "npm run check:path && expo prebuild",
    "prebuild:clean": "npm run check:path && expo prebuild --clean",
    "eas:configure": "eas build:configure",
    "build:ios": "npm run check:path && eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:development": "npm run check:path && eas build --profile development --platform ios",
    "postinstall": "patch-package",
    "check:path": "node scripts/check-path-safe.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.25",
    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/react-query": "^5.85.5",
    "expo": "~53.0.20",
    "expo-camera": "^16.1.11",
    "expo-dev-client": "^5.2.4",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.14.1",
    "react-native-vision-camera": "^4.5.0",
    "react-native-worklets-core": "^1.3.3",
    "vision-camera-code-scanner": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "patch-package": "^8.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
